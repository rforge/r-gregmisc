useDynLib(SASxport, .registration=TRUE)

export(
       assert,
       toSAS,
       lookup.xport,
       read.xport,
       write.xport
       )

S3method(toSAS,numeric)
S3method(toSAS,logical)
S3method(toSAS,character)
S3method(toSAS,factor)
S3method(toSAS,POSIXt)
S3method(toSAS,Date)
S3method(toSAS,default)


